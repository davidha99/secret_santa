<%= simple_form_for exchange_event, html: { class: "form family" } do |f| %>
  <%= form_error_notification(exchange_event) %>

  <%= f.input :year, html5: true, input_html: { autofocus: true } %>
  <%= link_to "Cancel", exchange_events_path, class: "btn btn--light" %>
  <%= f.submit class: "btn btn--secondary" %>
<% end %>
